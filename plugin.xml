                       target="jniLibs/armeabi-v7a/libwtbt828.so"/>
        <resource-file src="src/resources/android/x86/libAMapSDK_MAP_v6_9_4.so"
                       target="jniLibs/x86/libAMapSDK_MAP_v6_9_4.so"/>
        <resource-file src="src/resources/android/x86/libAMapSDK_NAVI_v6_9_1.so"
                       target="jniLibs/x86/libAMapSDK_NAVI_v6_9_1.so"/>
