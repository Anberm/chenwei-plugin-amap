                       target="jniLibs/arm64-v8a/libmp3decoder.so"/>
        <resource-file src="src/resources/android/arm64-v8a/librtbt828.so" target="jniLibs/arm64-v8a/librtbt828.so"/>
        <resource-file src="src/resources/android/arm64-v8a/libwtbt828.so" target="jniLibs/arm64-v8a/libwtbt828.so"/>
